#: .\venv\Lib\site-packages\django\contrib\flatpages\admin.py:13
msgid "Advanced options"
msgstr "Geavanceerde opties"

#: .\venv\Lib\site-packages\django\contrib\flatpages\apps.py:8
msgid "Flat Pages"
msgstr "Platte pagina's"

#: .\venv\Lib\site-packages\django\contrib\flatpages\forms.py:11
#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:9
msgid "URL"
msgstr "URL"

#: .\venv\Lib\site-packages\django\contrib\flatpages\forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr "Voorbeeld: ‘/about/contact/’. Zorg voor slashes aan het begin en eind."

#: .\venv\Lib\site-packages\django\contrib\flatpages\forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Deze waarde mag alleen letters, cijfers, punten, liggende streepjes, "
"streepjes, slashes of tildes bevatten."

#: .\venv\Lib\site-packages\django\contrib\flatpages\forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr "Voorbeeld: ‘/about/contact’. Zorg voor een slash aan het begin."

#: .\venv\Lib\site-packages\django\contrib\flatpages\forms.py:47
msgid "URL is missing a leading slash."
msgstr "In de URL ontbreekt een beginslash."

#: .\venv\Lib\site-packages\django\contrib\flatpages\forms.py:52
msgid "URL is missing a trailing slash."
msgstr "In de URL ontbreekt een eindslash."

#: .\venv\Lib\site-packages\django\contrib\flatpages\forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Platte pagina met URL %(url)s bestaat al voor website %(site)s"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:10
msgid "title"
msgstr "titel"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:11
msgid "content"
msgstr "inhoud"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:12
msgid "enable comments"
msgstr "opmerkingen toestaan"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:14
msgid "template name"
msgstr "sjabloonnaam"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Voorbeeld: ‘flatpages/contact_page.html’. Als dit niet is opgegeven, wordt "
"‘flatpages/default.html’ gebruikt."

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:23
msgid "registration required"
msgstr "registratie vereist"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Als dit is aangevinkt, kunnen alleen aangemelde gebruikers de pagina "
"bekijken."

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:29
msgid "sites"
msgstr "websites"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:33
msgid "flat page"
msgstr "platte pagina"

#: .\venv\Lib\site-packages\django\contrib\flatpages\models.py:34
msgid "flat pages"
msgstr "platte pagina's"
